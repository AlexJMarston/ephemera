{
  "name": "ephemera",
  "author": "Me",
  "description": "Ephemera Collection Tracker",
  "main": "dist/out-tsc/electron/electron-main.js",
  "private": true,
  "productName": "Ephemera",
  "version": "0.0.0",
  "scripts": {
    "build": "ng build",
    "build:electron": "tsc --project electron/tsconfig.json && ng build --configuration electron",
    "lint": "ng lint",
    "make": "electron-forge make",
    "package": "electron-forge package",
    "serve": "ng serve",
    "serve:electron": "ng serve --configuration electron",
    "start": "electron-forge start",
    "start:electron": "tsc --project electron/tsconfig.json && dotenvx run --env-file=.env.dev --env-file=.env.dev.served -- electron-forge start",
    "test": "ng test --code-coverage"
  },
  "dependencies": {
    "@angular/common": "^20.0.0",
    "@angular/compiler": "^20.0.0",
    "@angular/core": "^20.0.0",
    "@angular/forms": "^20.0.0",
    "@angular/platform-browser": "^20.0.0",
    "@angular/router": "^20.0.0",
    "@fortawesome/angular-fontawesome": "^2.0.1",
    "@fortawesome/free-brands-svg-icons": "^6.7.2",
    "@fortawesome/free-regular-svg-icons": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@ng-bootstrap/ng-bootstrap": "^19.0.0",
    "@popperjs/core": "^2.11.8",
    "bootstrap": "^5.3.6",
    "electron-squirrel-startup": "^1.0.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^20.0.3",
    "@angular/cli": "^20.0.3",
    "@angular/compiler-cli": "^20.0.0",
    "@angular/localize": "^20.0.0",
    "@dotenvx/dotenvx": "^1.45.1",
    "@electron-forge/cli": "^7.8.1",
    "@electron-forge/maker-squirrel": "^7.8.1",
    "@electron-forge/plugin-auto-unpack-natives": "^7.8.1",
    "@electron-forge/plugin-fuses": "^7.8.1",
    "@electron/fuses": "^1.8.0",
    "@types/jasmine": "~5.1.0",
    "angular-eslint": "20.1.1",
    "electron": "37.0.0",
    "electron-reloader": "^1.2.3",
    "eslint": "^9.29.0",
    "jasmine-core": "~5.7.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.8.2",
    "typescript-eslint": "8.34.1"
  }
}
